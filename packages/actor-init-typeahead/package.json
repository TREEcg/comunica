{
  "name": "@hdelva/actor-init-typeahead",
  "version": "1.18.1",
  "lsd:module": "https://example.org/actor-init-typeahead",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://example.org/actor-init-typeahead/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://example.org/actor-init-typeahead/^1.0.0/components/": "components/",
    "https://example.org/actor-init-typeahead/^1.0.0/config/": "config/"
  },
  "main": "index.js",
  "typings": "index",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bin": {
    "comunica-rdf-dereference": "./bin/run.js"
  },
  "files": [
    "components",
    "config",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@comunica/bus-http": "^1.18.1",
    "@comunica/bus-init": "^1.18.1",
    "@comunica/bus-rdf-dereference": "^1.18.1",
    "@comunica/bus-rdf-metadata": "^1.19.0",
    "@comunica/bus-rdf-parse": "^1.18.1",
    "@comunica/core": "^1.18.1",
    "@comunica/runner": "^1.18.1",
    "@comunica/runner-cli": "^1.18.1",
    "@hdelva/bus-rdf-score": "^1.0.0",
    "@hdelva/bus-tree-score": "^1.0.0",
    "n3": "^1.8.0",
    "rdf": "^4.1.0",
    "rdf-data-factory": "^1.0.4",
    "rdf-string": "^1.5.0",
    "tinyqueue": "^2.0.3"
  },
  "devDependencies": {
    "@comunica/mediator-race": "^1.18.1",
    "@types/rdf-js": "^4.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  }
}
