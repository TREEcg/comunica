{
  "name": "@treecg/bus-rdf-score",
  "version": "1.18.1",
  "description": "A comunica bus for scoring collections of RDF quads.",
  "lsd:module": true,
  "main": "index.js",
  "typings": "index",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/rdf-js": "*"
  },
  "peerDependencies": {
    "@comunica/core": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.18.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  }
}
