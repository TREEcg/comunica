{
  "name": "@hdelva/mediator-combine-sequence",
  "version": "1.18.1",
  "description": "A combine-sequence mediator",
  "lsd:module": "https://example.org/mediator-combine-sequence",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://example.org/mediator-combine-sequence/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://example.org/mediator-combine-sequence/^1.0.0/components/": "components/"
  },
  "main": "index.js",
  "typings": "index",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "peerDependencies": {
    "@comunica/core": "^1.0.0",
    "@comunica/mediator-combine-union": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.18.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  }
}
